<%= render 'layouts/shared/side_menu_owner'%>

<div class="body_class" style="margin-top: 2%;">


<table id='order-data-table' class="table align-middle mb-0 bg-white table-hover" >
  <thead class="bg-light">
    <tr class="table-active">
      <th>Order id</th>
      <th>User id</th>
      <th>Product Name </h1>
       <th>Email</th>
      <th>Product id</th>
      <th>Quantity</th>
      <th>Price</th>
      <th>Store name</th>
      <th>Purcahse date</th>  
    </tr>
  </thead>
</table>



<script type="text/javascript">
  jQuery('#order-data-table').dataTable({
    
      "processing": true,
      
      "ajax": {
          "url": $('#order-data-table').data('source')
      },
      "pagingType": "full_numbers",
      "columns": [
          {"data": "id"},
          {"data": "user_id"},
          {"data": "product_name"},
          {"data": "email"},
          {"data": "product_id"},
          {"data": "quantity"},
          {"data": "price"},
          {"data": "store_name"},
          {"data": "created_at"},
      ]
  })
  $.fn.dataTable.ext.errMode = 'throw';
</script>

